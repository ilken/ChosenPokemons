{
	"env": {
		"production": {
			"presets": [
				["es2015"],
				"stage-2",
				"react"
			],
			"plugins": [
				"react-html-attrs",
				"transform-decorators-legacy",
				"transform-class-properties",
			]
		},
		"development": {
			"presets": [
				["es2015", {"modules": false}],
				"stage-2",
				"react"
			],
			"plugins": [
				"react-html-attrs",
				"transform-decorators-legacy",
				"transform-class-properties",
				"react-hot-loader/babel"
			]
		}
	}
}
